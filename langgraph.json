{
  "$schema": "https://langgra.ph/schema.json",
  "dependencies": ["."],
  "graphs": {
    "business_plan": "./graphs/agent_graph.py:graph"
  },
  "env": ".env",
  "http": {
    "cors": {
      "allow_origins": ["http://127.0.0.1:5500", "http://localhost:5500"],
      "allow_origin_regex": "https?://(localhost|127\\.0\\.0\\.1)(:\\d+)?$",
      "allow_methods": ["*"],
      "allow_headers": ["*"],
      "expose_headers": ["*"],
      "allow_credentials": true,
      "max_age": 86400
    }
  }
}